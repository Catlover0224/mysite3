<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="board">
	<select id="getList" resultType="com.javaex.vo.BoardVO">
		<![CDATA[
			SELECT 
			    no,
			    title,
			    writer,
			    views,
			    boarddate,
			    content
			FROM board
		]]>
	</select>
</mapper>